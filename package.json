{
  "name": "@fellowapp/n8n-nodes-fellow",
  "version": "0.1.0",
  "description": "Fellow nodes for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://fellow.app",
  "author": {
    "name": "Fellow",
    "email": "devteam@fellow.app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fellowapp/n8n-nodes-fellow.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp --gulpfile gulpfile.cjs build:icons",
    "build:prod": "cross-env FELLOW_API_URL='https://fellow.app/graphql' tsc && gulp --gulpfile gulpfile.cjs build:icons",
    "build:staging": "cross-env FELLOW_API_URL='https://staging.fellow.co/graphql' tsc && gulp --gulpfile gulpfile.cjs build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FellowApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Fellow/FellowTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "^22.15.3",
    "@typescript-eslint/parser": "^8.31.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "globals": "^16.0.0",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
